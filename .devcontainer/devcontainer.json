{
    "settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
		"workbench.colorTheme": "Night Owl"
	},
    "extensions": [
		"sdras.night-owl",
		"ms-vscode.cpptools"
	],
	"forwardPorts": [
		3000
	],
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "ubuntu-20.04"
		}
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
}
